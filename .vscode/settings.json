  "git.pullTags": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.exclude": {
    "build": true,
    "dist": true,
    "node_modules": true,
    "public/client.min.js": true
  },
  "[typescript]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.formatOnType": false
  }
}
